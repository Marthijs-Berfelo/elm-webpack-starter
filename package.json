{
  "name": "elm-webpack-starter",
  "description": "Webpack setup for writing Elm apps",
  "version": "0.9.0",
  "license": "MIT",
  "author": "Marthijs Berfelo",
  "repository": {
    "type": "git",
    "url": "https://github.com/Marthijs-Berfelo/elm-webpack-starter"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "prebuild": "gulp dist-clean",
    "build": "webpack",
    "test": "elm-test",
    "prereinstall": "yarn add -D shx && shx rm -rf node_modules && npm uninstall -g elm",
    "reinstall": "npm run prereinstall && npm i -g elm && yarn install && gulp elm"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.5",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "elm": "~0.19.1-3",
    "elm-format": "^0.8.2",
    "elm-test": "^0.19.1",
    "elm-webpack-loader": "^6.0.0",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-elm": "^0.8.2",
    "gulp-run-command": "^0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.5.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "shx": "^0.3.2",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.1.0"
  }
}
